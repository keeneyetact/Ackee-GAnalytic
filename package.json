{
  "name": "ackee",
  "private": true,
  "version": "2.0.0-1",
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ],
  "description": "Self-hosted, Node.js based analytics tool for those who care about privacy",
  "main": "src/index.js",
  "keywords": [
    "server",
    "tracking",
    "analytics"
  ],
  "license": "MIT",
  "homepage": "https://github.com/electerious/Ackee",
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/Ackee.git"
  },
  "funding": {
    "type": "paypal",
    "url": "https://paypal.me/electerious"
  },
  "scripts": {
    "start": "node src/index.js",
    "start:dev": "NODE_ENV=development nodemon",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test": "nyc ava",
    "lint": "eslint '{src,test}/**/*.js'"
  },
  "eslintConfig": {
    "root": true,
    "extends": "@electerious/eslint-config"
  },
  "dependencies": {
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "ackee-tracker": "^4.0.0-1",
    "apollo-server-micro": "^2.16.1",
    "classnames": "^2.2.6",
    "date-fns": "^2.15.0",
    "date-fns-tz": "^1.0.10",
    "debounce-promise": "^3.1.2",
    "dotenv": "^8.2.0",
    "formbase": "^12.0.1",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.2.6",
    "graphql-tools": "^6.0.15",
    "human-number": "^1.0.5",
    "immer": "^7.0.7",
    "is-url": "^1.2.4",
    "micro": "^9.3.4",
    "microrouter": "^3.1.3",
    "mongoose": "^5.9.27",
    "node-fetch": "^2.6.0",
    "node-schedule": "^1.3.2",
    "normalize-url": "^5.0.0",
    "normalize.css": "^8.0.0",
    "permit": "^0.2.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^2.3.1",
    "react-fast-compare": "^3.2.0",
    "react-hotkeys-hook": "^2.2.2",
    "react-redux": "^7.2.1",
    "react-use": "^15.3.3",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "request-ip": "^2.1.3",
    "rosid-handler-js": "^13.0.0",
    "rosid-handler-sass": "^7.0.0",
    "s-ago": "^2.2.0",
    "shortid": "^2.2.15",
    "signale": "^1.4.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@electerious/eslint-config": "^1.3.4",
    "ava": "3.11.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-react-native": "^3.8.1",
    "mocked-env": "^1.3.2",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "test-listen": "^1.1.0"
  },
  "ava": {
    "verbose": true,
    "timeout": "20s",
    "environmentVariables": {
      "ACKEE_TRACKER": "custom name"
    }
  },
  "nodemonConfig": {
    "ext": "js,json,graphql",
    "ignore": [
      "data/*",
      "docs/*",
      "test/*",
      "src/ui/*"
    ]
  },
  "engines": {
    "node": ">= 10"
  }
}
